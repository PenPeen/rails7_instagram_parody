<%= render partial: "partial/post_validation_messages" %>
<%= form_with model: @post, data:{turbo: false} do |f| %>
  <%= f.label :caption %>
  <%= f.text_field :caption %>
  <%= f.fields_for :photos, @post.photos.build do |i| %>
    <%= i.file_field :image %>
  <% end %>
  <%= f.submit "投稿", class: "btn btn-primary" %>
<% end %>
